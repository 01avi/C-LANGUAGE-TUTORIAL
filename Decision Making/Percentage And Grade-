#include<stdio.h>
main()
{
	int phy, che, math, hindi, english;
	float per;
	printf("Enter Physics marks: -");
        scanf("%d", &phy);
        printf("Enter Chemistry marks: -");
        scanf("%d", &che);
        printf("Enter Mathmatics marks: -");
        scanf("%d", &math);
        printf("Enter Hindi marks: -");
        scanf("%d", &hindi);
        printf("Enter English marks: -");
        scanf("%d", &english);
  
	per = ( phy + che + math + hindi + english ) / 5;
	printf("Percentage is:- %.2f\n", per);
	if(per>=90) {
		printf("Grade A");
    
	} else if(per>=80) {
		printf("Grade B");
    
	} else if(per>=70) {
		printf("Grade C");
    
	}else if(per>=60) {
		printf("Grade D");
    
	} else {
	  printf("Grade F");
  }
}
